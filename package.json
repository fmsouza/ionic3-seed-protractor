{
  "name": "ionic-seed",
  "version": "1.0.0",
  "description": "Ionic seed",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fmsouza/ionic3-seed-protractor.git"
  },
  "scripts": {
    "build": "ionic-app-scripts build",
    "clean": "ionic-app-scripts clean",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "npm run build",
    "ionic:serve": "ionic serve",
    "ionic:lint": "npm run lint",
    "start": "npm run ionic:serve",
    "ios": "ionic cordova run ios",
    "android": "ionic cordova run android && adb logcat | grep CONSOLE",
    "test": "npm run unittest && npm run e2e",
    "unittest": "karma start ./test/karma.conf.js --single-run",
    "unittest-watch": "karma start ./test/karma.conf.js",
    "test-coverage": "karma start ./test/karma.conf.js --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",
    "appium": "appium"
  },
  "dependencies": {
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/compiler-cli": "5.1.3",
    "@angular/core": "5.1.3",
    "@angular/forms": "5.1.3",
    "@angular/http": "5.1.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "@ionic-native/app-version": "4.5.2",
    "@ionic-native/core": "4.5.2",
    "@ionic-native/keyboard": "4.5.2",
    "@ionic-native/splash-screen": "4.5.2",
    "@ionic-native/status-bar": "4.5.2",
    "@ionic/storage": "2.1.3",
    "cordova-android": "7.0.0",
    "cordova-ios": "4.5.4",
    "cordova-plugin-app-version": "0.1.9",
    "cordova-plugin-device": "2.0.1",
    "cordova-plugin-ionic-keyboard": "2.0.5",
    "cordova-plugin-ionic-webview": "1.1.16",
    "cordova-plugin-splashscreen": "5.0.1",
    "cordova-plugin-statusbar": "2.4.1",
    "cordova-plugin-whitelist": "1.3.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "rxjs": "5.5.6",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.7",
    "@types/jasmine": "2.8.3",
    "@types/node": "9.3.0",
    "angular2-template-loader": "0.6.2",
    "appium": "1.7.2",
    "html-loader": "0.5.4",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-coverage-istanbul-reporter": "1.3.3",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs2-launcher": "0.5.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.9",
    "null-loader": "0.1.1",
    "protractor": "5.2.2",
    "ts-loader": "3.2.0",
    "ts-node": "4.1.0",
    "typescript": "2.6.2",
    "wd": "1.5.0",
    "wd-bridge": "0.0.2"
  },
  "config": {
    "ionic_aot_write_to_disk": true
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-statusbar": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-device": {},
      "cordova-plugin-app-version": {}
    }
  }
}
