  "repository": {
    "type": "git",
    "url": "https://github.com/fmsouza/ionic2-seed.git"
  },
  "scripts": {
    "postinstall": "ionic build && ionic state restore",
    "build": "ionic-app-scripts build",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
