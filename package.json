    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.12",
    "angular2-template-loader": "0.6.2",
    "codelyzer": "^3.0.0-beta.4",
    "html-loader": "0.4.5",
    "jasmine": "2.5.3",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "null-loader": "0.1.1",
    "ts-loader": "2.0.3",
    "typescript": "2.2.2"
  },
